/*每个页面公共css */
page {
  width: 100vm;
  min-height: 100%;
  display: flex;
  font-size: 28rpx;
  background-color: #fff;
  box-sizing: border-box;
}
.container {
  width: 750rpx;
  padding: 20rpx;
  padding-bottom: 0 !important;
}
.pages {
  width: 750rpx;
}
.box {
  width: 100%;
}

.pages {
  position: relative;
}
.dynamic {
  display: flex;
  width: 100%;
  height: 220rpx;
  align-items: center;
  padding: 0 32rpx;
  box-sizing: border-box;
  background: url(data:image/png;base64,) !important;
}
.dynamic image {
  width: 180rpx;
  height: 180rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.dynamic .title {
  flex: 1;
  color: #000000;
  line-height: 66rpx;
}
.dynamic .title .des-name {
  font-size: 48rpx;
  line-height: 1.8;
  padding-bottom: 8rpx;
}
.dynamic .title .des-more {
  font-size: 32rpx;
  line-height: 1.2;
  color: #e99300;
  overflow: hidden !important;
  text-overflow: ellipsis !important;
  /* 超出部分省略号 */
  word-break: break-all !important;
  /* break-all(允许在单词内换行。) */
  display: -webkit-box !important;
  /* 对象作为伸缩盒子模型显示 */
  -webkit-box-orient: vertical !important;
  /* 设置或检索伸缩盒对象的子元素的排列方式 */
  -webkit-line-clamp: 2 !important;
  /* 显示的行数 */
}
.content {
  font-size: 32rpx;
  overflow: hidden;
}
.content video {
  width: 100%;
}
.content .content-c {
  margin: 10rpx 20rpx;
  word-break: break-all;
  font-size: 36rpx;
  line-height: 1.4;
  white-space: pre-line;
}
.content .content-i {
  width: 100%;
}
.content .content-i image {
  display: block;
  width: 100% !important;
}
.no-all {
  display: flex;
  margin: 0 20rpx;
}
.all {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  padding: 18rpx 0 12rpx;
  margin: 0 20rpx;
  border-bottom: 2rpx solid #acacac;
}
.all .option {
  display: flex;
  justify-content: flex-end;
}
.all .option .u-icon {
  margin-right: 16rpx;
}
.other {
  padding: 20rpx 20rpx 26rpx;
}
.other .other-item {
  display: flex;
  margin: 16rpx 0;
}
.other .other-item .other-pri {
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  margin-right: 16rpx;
}
.other .other-item .other-all {
  flex: 1;
}
.other .other-item .other-all .other-name {
  font-size: 34rpx;
  margin-bottom: 6rpx;
}
.other .other-item .other-all .other-say {
  color: #444444;
  white-space: pre-line;
  word-break: break-all !important;
  /* break-all(允许在单词内换行。) */
}
.other .other-item .other-all .other-bottom {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 10rpx 0;
}
.other .other-item .other-all .other-bottom .other-other {
  display: flex;
  background-color: #e0e0e0;
  color: #ff8000;
  padding: 4rpx 4rpx 4rpx 12rpx;
  border-radius: 20rpx;
  letter-spacing: 2rpx;
}
.cancel-input {
  position: absolute;
  z-index: 100;
  width: 750rpx;
  height: 100vh;
  left: 0;
  top: 0;
}
.input-box {
  position: fixed;
  width: 100%;
  padding: 0 1%;
  background-color: #f2f2f2;
  display: flex;
  z-index: 111150;
  bottom: 0rpx;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.input-box .textbox {
  width: 100%;
  min-height: 70rpx;
  margin-bottom: 10rpx;
}
.input-box .textbox .text-mode {
  width: 95%;
  min-height: 70rpx;
  display: flex;
  background-color: #fff;
  margin: 15rpx auto;
  border-radius: 40rpx;
}
.input-box .textbox .text-mode .box {
  width: 100%;
  padding: 0 30rpx;
  min-height: 70rpx;
  display: flex;
  align-items: center;
}
.input-box .textbox .text-mode .box textarea {
  width: 100%;
}
.input-box .textbox .text-mode .em {
  flex-shrink: 0;
  width: 80rpx;
  padding-left: 10rpx;
  height: 70rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.send {
  flex-shrink: 0;
  width: 150rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
}
.send .btn {
  width: 120rpx;
  height: 56rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #e89406;
  color: #fff;
  border-radius: 6rpx;
  font-size: 28rpx;
}
.re-title {
  font-size: 40rpx;
  color: #e99300;
  text-align: center;
  margin: 30rpx 20rpx 10rpx;
  padding-bottom: 30rpx;
  border-bottom: 2rpx solid #bababa;
}
.list-tiem {
  display: flex;
  padding: 10rpx 20rpx;
}
.list-tiem .ava {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.list-tiem .count {
  flex: 1;
  color: #333;
  word-break: break-all !important;
  /* break-all(允许在单词内换行。) */
}
.list-tiem .count .count-name {
  font-size: 34rpx;
  margin-bottom: 8rpx;
}
.list-tiem .count .count-comment {
  color: #444444;
}
.list-tiem .count .count-timer {
  text-align: right;
  font-size: 26rpx;
  color: #888586;
}
.re-sm-title {
  margin: 20rpx;
  color: #565656;
}
.send-box {
  width: 100%;
  height: 750rpx;
  padding: 120rpx 180rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}
.send-box .u-input {
  flex: 0 !important;
  border-radius: 20rpx;
  border: 2rpx solid #888586 !important;
}
.send-box .u-input__content {
  height: 80rpx;
}
.send-box .u-input__content .u-icon__img {
  width: 70rpx !important;
  height: 70rpx !important;
}
.send-box .send-ok {
  margin-top: 20rpx;
  font-size: 44rpx;
  text-align: center;
  width: 130rpx;
  height: 74rpx;
  line-height: 74rpx;
  background: #ffffff;
  border-radius: 20rpx;
  border: 2rpx solid #888586;
}
 .u-popup__content {
  border-radius: 30rpx 30rpx 0 0;
}
swiper {
  height: 750rpx;
}
swiper image {
  width: 100%;
  height: 100%;
}
.next {
  margin-top: 10rpx;
  width: 100%;
  color: #9a9698;
  text-align: center;
}
button {
  background-color: transparent;
  margin: 0;
  padding: 0;
}
button::after {
  border: 0;
}
.ua-box {
  margin-left: 14rpx;
}
.singleImg {
  display: block;
  width: 750rpx;
}

