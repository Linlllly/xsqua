/*每个页面公共css */
page {
  width: 100vm;
  min-height: 100%;
  display: flex;
  font-size: 28rpx;
  background-color: #fff;
  box-sizing: border-box;
}
.container {
  width: 750rpx;
  padding: 20rpx;
  padding-bottom: 0 !important;
}
.pages {
  width: 750rpx;
  position: relative;
}
.box {
  width: 100%;
}

.reg {
  position: relative;
  width: 100%;
  min-height: 1480rpx;
  background-color: rgba(0, 0, 0, 0.1);
}
.reg-title {
  width: 170rpx;
  height: 150rpx;
  box-sizing: border-box;
  padding: 36rpx 36rpx 0;
}
.reg-title .uicon-reload {
  background-color: rgba(0, 0, 0, 0.5);
  border-radius: 50%;
}
.reg-content {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}
.reg-content .content-item {
  box-sizing: border-box;
  width: 360rpx;
  padding: 46rpx;
}
.reg-content .content-item:nth-child(2n) {
  padding-left: 36rpx;
}
.reg-content .content-item:nth-child(2n + 1) {
  padding-right: 36rpx;
}
.reg-content .content-item:nth-child(1) {
  height: 556rpx;
}
.reg-content .content-item:nth-child(2) {
  height: 556rpx;
}
.reg-content .content-item:nth-child(3) {
  height: 706rpx;
}
.reg-content .content-item:nth-child(4) {
  height: 706rpx;
}
.btn {
  position: relative;
  color: white;
  background-color: rgba(0, 0, 0, 0.4);
  box-shadow: 0rpx 1rpx 0.5rpx 0rpx white inset;
}
.text {
  position: absolute;
  display: flex;
  width: 100%;
  left: 50%;
  top: 50%;
  -webkit-transform: translateX(-50%) translateY(-50%);
          transform: translateX(-50%) translateY(-50%);
  flex-direction: column;
  justify-content: center;
  text-align: center;
}
.local {
  position: absolute;
  width: 44rpx;
  height: 44rpx;
}
.bg-img {
  position: absolute;
  width: 100%;
  height: 1480rpx;
  z-index: -5;
}
.bg-img ._img {
  width: 100%;
  height: 100%;
}
.rooms {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.4);
}
.rooms .rooms-content {
  display: flex;
  flex-wrap: wrap;
  position: relative;
  align-content: flex-start;
  width: 664rpx;
  height: 1080rpx;
  background: rgba(255, 255, 255, 0.75);
  border-radius: 16rpx;
  margin: 166rpx auto 0;
  padding: 72rpx 35rpx 110rpx;
  box-sizing: border-box;
}
.rooms .rooms-content .uicon-close {
  position: absolute;
  top: 25rpx !important;
  left: 25rpx;
}
.rooms .rooms-content .rooms-name {
  position: absolute;
  top: 20rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  width: 90px;
  height: 25px;
  font-size: 18px;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #000000;
  line-height: 25px;
}
.rooms .rooms-content .rooms-street-name {
  width: 100%;
  height: 42px;
  font-size: 30px;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #f04f2f;
  line-height: 42px;
  text-align: center;
  margin: 6rpx 0;
}
.rooms .rooms-content .content-box {
  display: flex;
  flex-wrap: wrap;
  align-content: flex-start;
  width: 100%;
  height: 800rpx;
}
.rooms .rooms-content .content-box .rooms-item {
  width: 136rpx;
  height: 140rpx;
  margin: 10rpx 7rpx;
  color: #333;
  font-weight: 600;
  font-size: 36rpx;
  text-align: center;
  line-height: 140rpx;
}
.rooms .rooms-content .rooms-prev,
.rooms .rooms-content .rooms-next {
  position: absolute;
  bottom: 26rpx;
  width: 72rpx;
  height: 72rpx;
  background-size: 72rpx 72rpx;
}
.rooms .rooms-content .rooms-comfirm {
  position: absolute;
  bottom: 26rpx;
  width: 122rpx;
  height: 72rpx;
  line-height: 72rpx;
  text-align: center;
  left: 41%;
  border-radius: 19px 19px 19px 19px;
  border: 4rpx solid #fcd621;
  background-color: rgba(255, 255, 255, 0.7);
}
.rooms .rooms-content .rooms-prev {
  background: url(data:image/png;base64,) no-repeat;
  left: 27%;
}
.rooms .rooms-content .rooms-next {
  background: url(data:image/png;base64,) no-repeat;
  right: 27%;
}
.room-yes {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 136rpx 140rpx;
}
.room-no {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 136rpx 140rpx;
}
.people {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.4);
}
.people .people-content {
  display: flex;
  flex-wrap: wrap;
  position: relative;
  align-content: flex-start;
  width: 620rpx;
  height: 1200rpx;
  background: rgba(255, 255, 255, 0.9);
  border-radius: 30rpx;
  margin: 156rpx auto 0;
  padding: 30rpx 50rpx 20rpx;
  box-sizing: border-box;
}
.people .people-content .title {
  display: flex;
  justify-content: space-between;
  width: 100%;
  height: 90rpx;
}
.people .people-content .title-left {
  display: flex;
}
.people .people-content .title-left .wenzi {
  font-size: 44rpx;
  line-height: 1.8;
  padding-bottom: 8rpx;
  border-bottom: 4rpx solid #727272;
}
.people .people-content .icon-box {
  width: 90rpx;
  height: 100%;
  padding-top: 20rpx;
  padding-left: 10rpx;
}
.people .people-content .icon-box .uicon-reload {
  top: 0 !important;
  left: 0 !important;
  background-color: transparent;
  padding: 0;
}
.people .people-content .icon-box .uicon-close {
  margin-left: 40rpx;
}
.people .people-item {
  display: flex;
  width: 100%;
  height: 130rpx;
  align-items: center;
  padding: 10rpx 0;
  box-sizing: border-box;
  border-bottom: 2rpx solid #a6a6a6;
}
.people .people-item image {
  width: 90rpx;
  height: 90rpx;
  margin: 0 10rpx;
  border-radius: 50%;
}
.people .people-item .des {
  flex: 1;
  display: flex;
  flex-direction: column;
}
.people .people-item .des .des-room {
  font-size: 32rpx;
}
.people .people-item .des .des-say {
  flex: 1;
  font-size: 28rpx;
  overflow: hidden !important;
  text-overflow: ellipsis !important;
  /* 超出部分省略号 */
  word-break: break-all !important;
  /* break-all(允许在单词内换行。) */
  display: -webkit-box !important;
  /* 对象作为伸缩盒子模型显示 */
  -webkit-box-orient: vertical !important;
  /* 设置或检索伸缩盒对象的子元素的排列方式 */
  -webkit-line-clamp: 1 !important;
  /* 显示的行数 */
}

